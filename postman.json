    {
      "name": "Ingest (Agent Token)",
      "event": [
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "pm.test('Status is 200', function () {",
              "  pm.response.to.have.status(200);",
              "});",
              "",
              "const data = pm.response.json();",
              "pm.test('Ingest response has expected fields', function () {",
              "  pm.expect(data).to.have.property('ok', true);",
              "  pm.expect(data).to.have.property('server_id');",
              "  pm.expect(data).to.have.property('alerts_created');",
              "  pm.expect(data).to.have.property('logs_inserted');",
              "  pm.expect(data.alerts_created).to.be.an('array');",
              "  pm.expect(data.logs_inserted).to.be.a('number');",
              "  pm.expect(data.logs_inserted).to.be.at.least(0);",
              "});"
            ]
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{agentToken}}"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"ts\": \"2026-01-01T00:00:00Z\",\n  \"host\": {\n    \"hostname\": \"demo\",\n    \"os\": \"Linux\",\n    \"os_release\": \"6.8\",\n    \"machine\": \"x86_64\",\n    \"primary_ip\": \"10.0.0.10\",\n    \"ip_addresses\": [\"10.0.0.10\", \"172.17.0.1\"],\n    \"domains\": [\"api.example.com\", \"app.example.com\"]\n  },\n  \"metrics\": {\n    \"cpu_percent\": 12.3,\n    \"ram_percent\": 55.1,\n    \"disk_percent\": 70.2,\n    \"loadavg\": [0.12, 0.22, 0.30],\n    \"net_bytes_sent\": 123,\n    \"net_bytes_recv\": 456\n  },\n  \"docker\": {\n    \"containers\": [\n      {\n        \"id\": \"abc123\",\n        \"image\": \"nginx:latest\",\n        \"name\": \"nginx\",\n        \"status\": \"Up 1 hour\"\n      },\n      {\n        \"id\": \"def456\",\n        \"image\": \"redis:7\",\n        \"name\": \"redis\",\n        \"status\": \"Restarting (1)\"\n      }\n    ],\n    \"events\": [\n      {\n        \"Action\": \"restart\",\n        \"Actor\": {\n          \"Attributes\": {\n            \"name\": \"redis\"\n          }\n        }\n      }\n    ]\n  },\n  \"logs\": {\n    \"systemd\": {\n      \"nginx\": [\"Started nginx\"],\n      \"ssh\": [\"Accepted publickey\"],\n      \"system\": [\"[syslog] kernel: nf_conntrack table full\"]\n    },\n    \"nginx\": [\n      \"[error.log] 2026/01/01 00:00:00 [error] 1#1: *1 connect() failed\",\n      \"[access.log] 10.0.0.2 - - [01/Jan/2026:00:00:00 +0000] \\\"GET /health HTTP/1.1\\\" 200 2\"\n    ],\n    \"docker\": [\n      \"[redis] CrashLoopBackOff detected\",\n      \"[api] Connection timeout to database\"\n    ]\n  }\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/v1/ingest",
          "host": [
            "{{baseUrl}}"
