              ]
            },
            "description": "Lists servers owned by the authenticated user."
          },
          "response": []
        },
        {
          "name": "Get Server Details",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status is 200', function () {",
                  "  pm.response.to.have.status(200);",
                  "});",
                  "",
                  "const data = pm.response.json();",
                  "",
                  "pm.test('Has ops identity fields', function () {",
                  "  pm.expect(data).to.have.property('primary_ip');",
                  "  pm.expect(data).to.have.property('ip_addresses');",
                  "  pm.expect(data).to.have.property('domains');",
                  "  pm.expect(data.ip_addresses).to.be.an('array');",
                  "  pm.expect(data.domains).to.be.an('array');",
                  "});",
                  "",
                  "pm.test('Has docker and logs fields', function () {",
                  "  pm.expect(data).to.have.property('docker_containers');",
                  "  pm.expect(data).to.have.property('log_sources');",
                  "  pm.expect(data).to.have.property('recent_logs');",
                  "  pm.expect(data.docker_containers).to.be.an('array');",
                  "  pm.expect(data.recent_logs).to.be.an('array');",
                  "});",
                  "",
                  "pm.test('Has agent capabilities and heartbeat fields', function () {",
                  "  pm.expect(data).to.have.property('agent_capabilities');",
                  "  pm.expect(data).to.have.property('last_heartbeat_at');",
                  "  pm.expect(data).to.have.property('heartbeat_status');",
                  "  pm.expect(data.agent_capabilities).to.be.an('object');",
                  "  if (data.heartbeat_status !== null && data.heartbeat_status !== undefined) {",
                  "    pm.expect(data.heartbeat_status).to.be.a('string');",
                  "  }",
                  "});",
                  "",
                  "if (Array.isArray(data.recent_logs) && data.recent_logs.length > 0) {",
                  "  pm.test('Recent logs item structure is valid', function () {",
                  "    const item = data.recent_logs[0];",
                  "    pm.expect(item).to.have.property('ts');",
                  "    pm.expect(item).to.have.property('source');",
                  "    pm.expect(item).to.have.property('level');",
                  "    pm.expect(item).to.have.property('message');",
                  "  });",
                  "  pm.test('Recent logs source/level values look valid', function () {",
                  "    const allowedSources = ['systemd', 'nginx', 'docker', 'unknown'];",
                  "    const allowedLevels = ['info', 'warn', 'error', 'unknown'];",
                  "    for (const item of data.recent_logs) {",
                  "      pm.expect(allowedSources).to.include(item.source);",
                  "      pm.expect(allowedLevels).to.include(item.level);",
                  "    }",
                  "  });",
                  "  pm.test('log_sources summary is consistent', function () {",
                  "    pm.expect(data.log_sources).to.be.an('object');",
                  "    const total = Object.values(data.log_sources).reduce((sum, value) => sum + Number(value || 0), 0);",
                  "    pm.expect(total).to.eql(data.recent_logs.length);",
                  "  });",
                  "} else {",
                  "  console.warn('No recent_logs yet. Wait for agent ingestion cycle.');",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{supabaseAccessToken}}"
