          "name": "Create Server",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "if (pm.response.code >= 200 && pm.response.code < 300) {",
                  "  const data = pm.response.json();",
                  "  if (data.server_id) pm.collectionVariables.set('serverId', data.server_id);",
                  "  if (data.agent_token) pm.collectionVariables.set('agentToken', data.agent_token);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{supabaseAccessToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"{{serverName}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/v1/servers",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "v1",
                "servers"
              ]
            },
            "description": "Creates a server for the authenticated user, returns one-time agent token and install command. Stores `serverId` and `agentToken` in collection variables."
          },
          "response": []
        },
        {
          "name": "List Servers",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{supabaseAccessToken}}"
              }
            ],
            "url": {
